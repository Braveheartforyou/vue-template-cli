{
  "name": "vue-template-cli",
  "version": "0.1.0",
  "description": "vue-template-cli init vue project",
  "bin": {
    "vuetemplate-init": "bin/vue-template-init"
  },
  "scripts": {
    "complie": "npx babel src -d dist --presets es2015",
    "watch": "npm run complie -- --watch",
    "postinstall": "vuetemplate config set"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Braveheartforyou/vue-template-cli.git"
  },
  "author": "blackdou",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue-cli",
    "vue-template"
  ],
  "bugs": {
    "url": "https://github.com/Braveheartforyou/vue-template-cli/issues"
  },
  "homepage": "https://github.com/Braveheartforyou/vue-template-cli#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-env": "^2.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "download-git-repo": "^2.0.0",
    "ini": "^1.3.5",
    "inquirer": "^6.5.0",
    "log-symbols": "^3.0.0",
    "ora": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-angular": "^9.1.1",
    "babel-core": "^6.0.20",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "logger": "0.0.1",
    "standard-version": "^8.0.2",
    "tildify": "^2.0.0",
    "user-home": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
